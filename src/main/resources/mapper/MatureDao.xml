<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cdl.demo.dao.MatureDao">

    <select id="getMatureByTime" resultType="com.cdl.demo.domain.Mature">
        SELECT news_id,user_id,grouping,comment,insert_time;
        FROM mature
        WHERE user_id = #{userId} and insert_time BETWEEN #{startTime} AND #{endTime}
    </select>

    <insert id="sendMature" parameterType="com.cdl.demo.domain.Mature">
        INSERT INTO mature(news_id,user_id,grouping,comment,insert_time)
        VALUES (#{newsId}, #{userId},#{grouping},#{comment},#{insertTime})
    </insert>

</mapper>